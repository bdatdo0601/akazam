{
    "name": "akazam",
    "version": "0.0.1",
    "description": "Informational Database of things",
    "main": "index.js",
    "repository": "git@github.com:bdatdo0601/akazam.git",
    "author": "Dat Bac Do <dat.b.do@gmail.com>",
    "license": "MIT",
    "private": false,
    "pre-commit": [
        "generate-mock",
        "test",
        "coveralls"
    ],
    "scripts": {
        "generate-mock": "babel-node __mock__/index.js",
        "start": "DEBUG=Akazam:* babel-node src/index.js",
        "start-watch": "DEBUG=Akazam:* nodemon src/index.js --exec babel-node",
        "test": "nyc --reporter=html --reporter=text --per-file mocha --recursive --require @babel/register",
        "coveralls": "nyc report --reporter=text-lcov | coveralls",
        "clean": "rm -rf build && mkdir build",
        "build-server": "babel -d ./build ./src -s --copy-files",
        "build": "yarn clean && yarn build-server",
        "start-production": "NODE_ENV=production node ./build/index.js"
    },
    "devDependencies": {
        "@babel/cli": "^7.1.0",
        "@babel/core": "^7.1.0",
        "@babel/node": "^7.0.0",
        "@babel/plugin-proposal-class-properties": "^7.1.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
        "@babel/plugin-transform-spread": "^7.0.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.0.0",
        "babel-preset-env": "^1.7.0",
        "chai": "^4.1.2",
        "chai-as-promised": "^7.1.1",
        "chai-like": "^1.1.1",
        "coveralls": "^3.0.2",
        "eslint": "^5.6.0",
        "eslint-config-prettier": "^3.0.1",
        "eslint-plugin-prettier": "^3.0.0",
        "faker": "^4.1.0",
        "json-schema-faker": "^0.5.0-rc15",
        "mkdirp": "^0.5.1",
        "mocha": "^5.2.0",
        "mocha-lcov-reporter": "^1.3.0",
        "nodemon": "^1.18.4",
        "nyc": "^13.0.1",
        "pre-commit": "^1.2.2",
        "prettier": "^1.14.3"
    },
    "dependencies": {
        "debug": "^4.0.1",
        "dotenv": "^6.0.0",
        "lodash": "^4.17.11",
        "mongoose": "^5.2.16",
        "parse-address": "^1.0.0"
    }
}
